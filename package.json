{
  "name": "pakeaja-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/check-env.js && next build",
    "build:no-check": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check-strict": "tsc --noEmit",
    "db:types": "npx supabase gen types typescript --project-id bemrgpgwaatizgxftzgg > src/types/database.types.ts",
    "db:push": "npx supabase db push",
    "db:pull": "npx supabase db pull",
    "db:reset": "npx supabase db reset",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:smoke": "playwright test tests/basic-functionality.test.ts",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:scan": "npm audit --audit-level=high --production",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --config=./lighthouse.config.js",
    "performance": "npm run lighthouse && npm run analyze:bundle",
    "pre-commit": "npm run lint && npm run type-check && npm run format:check && npm run test:unit",
    "pre-push": "npm run test:ci && npm run security:scan",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "clean": "rm -rf .next out dist coverage .lighthouseci",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "health-check": "curl -f http://localhost:3000/api/health || echo 'Health check endpoint not available'",
    "prepare": "husky install"
  },
